package interviewer_Tasks;

public class VariableCheck {
//	void show(byte b) {
//		System.out.println("byte method");
//	}
//	void show(short b) {
//		System.out.println("short method");
//	}
	void show(int b) {
		System.out.println("int method");
	}
	void show(Integer b) {
		System.out.println("Integer method");
	}
	void show(Byte b) {
		System.out.println("byte class method");
	}
	void show(long b) {
		System.out.println("byte class method");
	}
	void show(double b) {
		System.out.println("byte class method");
	}
	
	public static void main(String[] args) {
		VariableCheck d=new VariableCheck();
		d.show((byte)129);
	}

}
