import java.util.Scanner;

public class XyzRatio {
	public static void main(String[] args) {
		int x=0,y=0;
		Scanner sc=new Scanner(System.in);
		String s=sc.nextLine();
		char[] a=s.toCharArray();
		for(int i=0;i<s.length();i++) {
			if(a[i]=='z')
				x++;
			else if(a[i]=='o')
				y++;
		}
		System.out.println(x+" "+y);
	}

}
