package collectionConcepts;

import java.util.TreeSet;

public class ComparableComparator implements Comparable {
	int empId;
	String empName;
	ComparableComparatorEmployee(String name,int eid){
		this.empId=eid;
		this.empName=name;
	}
	@Override
	public String toString() {
		return empName+"--"+empId;
	}

	
	@Override
	public int compareTo(Object o) {
		int eid1=this.empId;
		ComparableComparatorEmployee e=(ComparableComparatorEmployee)o;
		int eid2=e.empId;
		if(eid1<eid2)
			return -1;
		else if(eid1>eid2)
			return +1;	
		else
			return 0;
	}
}
class Implementation{
	public static void main(String[] args) {
		ComparableComparatorEmployee e1=new ComparableComparatorEmployee("shivam", 21154);
		ComparableComparatorEmployee e2=new ComparableComparatorEmployee("shashank", 21153);
		ComparableComparatorEmployee e3=new ComparableComparatorEmployee("shubham", 21155);
		ComparableComparatorEmployee e4=new ComparableComparatorEmployee("abcd", 21120);
		TreeSet t=new TreeSet();
		t.add(e1);
		t.add(e2);
		t.add(e3);
		t.add(e4);
		System.out.println(t);
	}	
} 
